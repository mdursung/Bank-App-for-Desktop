package bank.model.cards;

public class CreditCard extends Card {
	
	protected double limit;
	
    public CreditCard(String lastDate, int limit){
    	super(lastDate);
    	this.limit = limit;
    }

    
    public double getLimit() {
        return limit;
    }

    public void setLimit(double limit) {
        this.limit = limit;
    }
}
