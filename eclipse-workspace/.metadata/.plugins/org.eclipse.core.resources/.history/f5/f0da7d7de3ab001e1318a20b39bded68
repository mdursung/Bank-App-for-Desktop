package cards;

public class BankCard extends Card {

	protected double balance;

    public BankCard(){
        super();
    }

    public BankCard(String card_number, String CVC, String last_date, double balance){
    	super(card_number, CVC, last_date);
    	this.balance = balance;
    }

    public double getBalance() {
        return balance;
    }

    public void setBalance(double balance) {
        this.balance = balance;
    }
}
