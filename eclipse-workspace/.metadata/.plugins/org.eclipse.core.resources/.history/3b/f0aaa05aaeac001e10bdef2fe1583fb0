package user;

import accounts.*;

public class Employeer extends User {

    protected String rank;
    protected Account account;
    
    Employeer(String rank, String name, String lastName, String TC, String gender, int age){
    	super(name, lastName, TC, gender, age);
    	this.rank = rank;
    }

    public boolean accept(){
        return true;
    }

    public boolean refuse(){
        return false;
    }


}
