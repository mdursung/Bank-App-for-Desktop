package user;


public asbtract class User implements Citizen{

    protected String name;
    protected String lastName;
    protected String TC;
    protected String gender;
    protected int age;

    User(String name, String lastName, String TC, String gender, int age){
    	this.name=name;
    	this.lastName=lastName;
    	this.TC=TC;
    	this.gender=gender;
    	this.age=age;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getLastname() {
        return lastName;
    }

    public void setLastname(String lastName) {
        this.lastName = lastName;
    }

    public int getAge() {
        return age;
    }

    public void setAge(int age) {
        this.age = age;
    }

    public String getTC() {
        return TC;
    }

    public void setTC(String TC) {
        this.TC = TC;
    }

    public String getGender() {
        return gender;
    }

    public void setGender(String gender) {
        this.gender = gender;
    }
}
