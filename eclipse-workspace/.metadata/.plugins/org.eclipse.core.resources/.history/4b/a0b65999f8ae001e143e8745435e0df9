package bank.user;

import accounts.*;

public class Employeer extends User {

	
    protected String rank; //yetki seviyesi
    protected Account account;
    
    public Employeer(String rank, String name, String lastName, String TC, String gender, int age){
    	super(name, lastName, TC, gender, age);
    	this.rank = rank;
    }

    //gelen istekler için kabul ve ret fonksiyonları
    public boolean accept(){
        return true;
    }

    public boolean refuse(){
        return false;
    }


}
