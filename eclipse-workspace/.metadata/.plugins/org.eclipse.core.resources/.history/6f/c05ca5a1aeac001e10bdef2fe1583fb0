package cards;

public class Card {

	protected int cardNumber;
	protected int CVC;
	protected String last_date;
	public static int cardCount = 0;
	public static int CVCCount = 0;

    public Card(){
    	this("30.01.2034");
    }

    public Card(String last_date){
        this.cardNumber = cardCount++;
        this.CVC = CVCCount++;
        this.last_date = last_date;
    }


}
